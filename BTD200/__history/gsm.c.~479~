               if((!stricmp(msgP3,"WL")) || (!stricmp(msgP3,"RG")) || (!stricmp(msgP3,"TM")) || (!stricmp(msgP3,"RDR")) || (!stricmp(msgP3,"RDRR"))){
                  strcpy(temp,msgP3);
