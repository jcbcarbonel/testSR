
#int_ext1
void ext_isr1(void)
{
   EXT_FLAG=1;
   strcpy(SMS_RCV,RXD_DATA);
}

void clear_inputs()
{
   int ii=0;
   for(ii=0;ii<60;ii++)
      fprintf(SERIAL_INT, "AT+CMGD=%i\r", ii);
}

void getGSM_Strength()
{
   char *statx=NULL;
   char *statcat=NULL;
   
   strcpy(DESIRED_RESPONSE, "+CSQ: ");
   clear_buffer();
   delay_ms(200);
   fprintf(SERIAL_INT,"AT+CSQ\r");
   statx = GetResponse();
   strcpy(TOKEN,",");
   statcat = strtok(statx,TOKEN);
   strcpy(funcReturnVar,statcat);
}

