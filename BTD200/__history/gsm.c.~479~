         else if(!stricmp(msgP1,"SENDINGTIME")) //SENDING TIME ------> SENDINGTIME <space> <PASSWORD> <SENDING TIME>
         {                                     
            if (msgP3)
            {
               AddDataEEprom(msgP3,EEPROM_SENDING_TIME); 
