               strcpy(ASTISERVERNUM,NULL);
               //fprintf(PC,"%i\n",twoservers);
               return 1;
            }
         }
//!         else if(!stricmp(msgP1,"IPADD")) //IPADD ------> IPADD <space> <PASSWORD> <IPADD>
//!         {
//!            if (msgP3)
//!            {
//!               AddDataEEprom(msgP3,EEPROM_SERVER_IP);
