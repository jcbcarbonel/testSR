         else if(!stricmp(msgP1,"DATETIME")) //DATE AND TIME --------> DATETIME <space> PASSWORD <space> <"YY/MM/DD,HH:MM:SS">
         {
            if (msgP3)
            {
               strcpy(timeSet,msgP3);
