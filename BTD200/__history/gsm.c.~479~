         else if(!stricmp(msgP1,"SATENABLE")) //SATELLITE ENABLE ------> SAT_EN <space> <PASSWORD> <space> <VALUE>
         {
            if (msgP3)
            {
               strcpy(temp,msgP3);
               writeCharEE(temp[0],EEPROM_SATELLITE_STATUS);
               SATELLITE_FLAG = atoi(temp);
