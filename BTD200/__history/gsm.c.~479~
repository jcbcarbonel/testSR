                  strcpy(SENSORCONFIG,temp);
                  SensorInitialize();
                  sprintf(SMS_TOBE_SENT,"Type-%s\n",SENSORCONFIG);
   //!               strcpy(SMS_TOBE_SENT,"SENSORCONFIG =");
   //!               strcat(SMS_TOBE_SENT,SENSORCONFIG);
