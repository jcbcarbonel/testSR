         else if(!stricmp(msgP1,"SENSORPOLL")) //SENSORPOLL ------> SENSORPOLL <space> <PASSWORD>
         {            
            sensorPoll();
            strcpy(SMS_TOBE_SENT, REALDATA);
            /*
            if (SENSORTYPE=='W')
            {
               //strDavies(0); //-->for davies
