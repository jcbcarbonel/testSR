void testDTR()
{
   int x;
   int y=0;
   if (DEBUGMODE != IN_DEBUG_MENU)
   {
      if (input(pin_USB_STAT))
      {
         for (x=0;x<=60;x++)
         {
            if (input(pin_USB_STAT)) 
               y++;
            else
               return;
            delay_ms(25);
         }
      }
      if (y >= 50)
         DEBUGMODE = USB_DETECTED;
      else
         DEBUGMODE = NO_USB;
   }
}

