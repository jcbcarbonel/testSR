void initLTE()
{
   strcpy(DESIRED_RESPONSE, "OK");
   clear_InterruptGSM();
   delay_ms(200);
   fprintf(GSM,"init");
   if(GetResponse())
      fprintf(USB,"LTE Initialized\n");
}


void igtLTE()
{
   strcpy(DESIRED_RESPONSE, "OK");
   clear_InterruptGSM();
   delay_ms(200);
   fprintf(GSM,"igt");
   if(GetResponse())
      fprintf(USB,"LTE Ignited\n");
      initLTE();
}

char signalLTE()
{
   char *signal=NULL;
   //strcpy(DESIRED_RESPONSE, "OK");
   clear_InterruptGSM();
   //delay_ms(200);
   fprintf(GSM,"signal");
   delay_ms(200);
   signal = GetResponse();
   fprintf(USB, signal);
   //return signal;
}
