int1 dataPresent_hmp155()
{
   int16 timeOut=0;
   
   while(!kbhit(RS485_HMP155)&&++timeOut<50000)
   { delay_us(100); }
   if(!kbhit(RS485_HMP155))
      return 0;
   else
      return 1;
}

void parseHMPData()
{
   char strx[25]={};
   char *ptr;
   char token_[3]={};
   char temperature[6]={};
   char humidity[6]={};
   int x=0;
   
   strcpy(token_," ");
   strcpy(strx,"RH= 35.5 %RH T= 23.0 'C");
   ptr = strtok(strx, token_);
   fprintf(PC,"\r");
   while (x < 5)
   {
      ptr = strtok(0,token_);
      fprintf(PC,"x=%i ptr=%s\r",x,ptr);
      if (x==0)
         strcpy(humidity,ptr);
      if (x==3)
         strcpy(temperature,ptr);
      x++;
   }
}

// sample output--> RH= 35.5 %RH T= 23.0 'C
void extractDataHMP155()
{
   int1 boolx=0;
   int1 dp=0;
   int i=0;
   int x=0;
   char cx[55]={};
   char chx;
   int retry=0;
   
   delay_ms(100);
   writeToRS485();
   delay_ms(5);
   fprintf(RS485_HMP155,"SEND 1\r");
   readFromRS485();
   dp = dataPresent_hmp155();
   if (dp == 1)
   {
      if(kbhit(RS485_HMP155))
      {
         while(i<35)
         {
            cx[i]=fgetc(RS485_HMP155);
            if ((boolx == 0) && (cx[i] == 'R'))
               boolx = 1;
            if ((boolx == 1) && (cx[i] == 'C'))
               break;
            if (boolx == 1)            
               i++;
            
         }
      }         
      while(x<=i)
      {
         fprintf(PC,"%c",cx[x]);
         x++;
      }
      fprintf(PC,"\n");
   }
   else
   {
      strcpy(funcReturnVar,"HMP155_err");
   }
   
}
